{
	"consul": {
    "endpoint": "http://localhost:8500",
    "service": "operations-service",
    "address": "localhost",
    "port": "5004",
    "pingEnabled": false,
    "pingEndpoint": "ping",
    "pingInterval": 5,
    "deregisterInterval": 10
  },
	"metrics": {
    "database": "dshop",
    "uri": "http://localhost:8086",
    "env": "local",
    "interval": 5,
    "enabled": true
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "mongo": {
    "connectionString": "mongodb://localhost:27017",
    "database": "dshop-operations-service",
    "seed": false
  },
	"rawRabbit": {
		"username": "guest",
		"password": "guest",
		"virtualHost": "/",
		"port": 5672,
		"hostnames": ["localhost"],
		"requestTimeout": "00:00:10",
		"publishConfirmTimeout": "00:00:01",
		"recoveryInterval": "00:00:10",
		"persistentDeliveryMode": true,
		"autoCloseConnection": true,
		"automaticRecovery": true,
		"topologyRecovery": true,
		"exchange": {
				"durable": true,
				"autoDelete": false,
				"type": "Topic"
		},
		"queue": {
				"autoDelete": false,
				"durable": true,
				"exclusive": false
		}
	}
}